  "scripts": {
    "get-contracts": "rsync -avm --include='*/' --include='*.sol' --exclude='*' ../../../../precompiles/ ./contracts/",
    "clean-contracts": "rm -rf ./contracts/*",
    "test-ganache": "yarn truffle test",
    "test-evmos": "yarn clean-contracts && yarn get-contracts && yarn truffle test --network evmos"
  },
  "devDependencies": {
    "truffle-assertions": "^0.9.2"
  }
}
